{
  "manifest_version": 3,
  "name": "Heavenly Beauty",
  "version": "1.0",
  "description": "Heavenly Beauty is a chrome extension that will display some grid data",
  "permissions": ["activeTab", "scripting", "http://localhost:8080/"],
  "action": {
    "default_popup": "root.html",
    "default_icon": {
      "16": "grid-icon.jpeg"
    }
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; connect-src ws://localhost:8080/;"
  }
}